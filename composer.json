{
    "name": "launchkey/launchkey",
    "description": "Official PHP library for LaunchKey Authentication",
    "type": "library",
    "keywords": ["authentication", "launchkey", "security"],
    "homepage": "https://github.com/launchkey/launchkey-php",
    "license": "MIT",
    "authors": [
        {
            "name": "LaunchKey",
            "email": "developers@launchkey.com",
            "homepage": "https://launchkey.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://launchkey.desk.com/",
        "wiki": "https://launchkey.com/docs/api/overview",
        "source": "https://github.com/launchkey/launchkey-php"
    },
    "require": {
        "php": ">= 5.3.0",
        "ext-curl": "*",
        "guzzle/guzzle": "~3.0",
        "phpseclib/phpseclib": ">= 0.3.5"
    },
    "autoload": {
        "psr-0": {
            "LaunchKey": "lib",
            "LaunchKey\\Tests": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "~0.9"
    },
    "config": {
        "bin-dir": "bin"
    }
}
