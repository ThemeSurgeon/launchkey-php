{
  "name": "launchkey/launchkey",
  "description": "Official PHP library for LaunchKey Authentication",
  "type": "library",
  "keywords": [
    "authentication",
    "launchkey",
    "security"
  ],
  "homepage": "https://github.com/launchkey/launchkey-php",
  "license": "MIT",
  "authors": [
    {
      "name": "LaunchKey",
      "email": "developers@launchkey.com",
      "homepage": "https://launchkey.com",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://launchkey.desk.com/",
